{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "rules": {
        "strict": 0,
        "no-console": "warn"
    },
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": false,
        "codeFrame": true
    },
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "jest": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}